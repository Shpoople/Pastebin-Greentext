{
"name":"Pastebin Greentext parser",
"description":"Formats greentext on pastebin and displays it in an easy to read page",
"version":"1.0.4",
"manifest_version":2,
"icons": { "16": "icon/16.png",
           "48": "icon/48.png",
          "128": "icon/128.png" },
"content_scripts": [
    {
      "matches": ["http://pastebin.com/*","https://pastebin.com/*"],
      "js": ["thirdParty/jquery-3.1.1.min.js", "script.js"],
      "css" : ["style.css"]
    }
  ]
}
